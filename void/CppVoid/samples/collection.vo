class Collection -> (items)
Collection.each -> (callback) => {
    for (item in self.items) -> callback <- (item)
    return self
}
Collection.map -> (callback) => {
    i -> 0
    while (i < len <- (items), i -> i + 1) -> self.items[i] -> callback <- (item)
    return self
}

data -> Collection([1, 2, 3, 4, 5])
